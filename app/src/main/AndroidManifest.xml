<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.colab">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MonitoringActivity"
            android:theme="@style/NoActionBar"></activity>
        <activity
            android:name=".DetailInventoryActivity"
            android:theme="@style/NoActionBar" />
        <activity
            android:name=".Inventory"
            android:theme="@style/NoActionBar" />
        <activity
            android:name=".login.MainActivity"
            android:theme="@style/NoActionBar" />
        <activity
            android:name=".login.RegisterActivity"
            android:theme="@style/NoActionBar" />
        <activity android:name=".util.Server" />
        <activity android:name=".util.AppController" />
        <activity
            android:name=".login.LoginActivity"
            android:theme="@style/NoActionBarWhite" />
        <activity
            android:name=".login.Splash"
            android:theme="@style/NoActionBarWhite">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>